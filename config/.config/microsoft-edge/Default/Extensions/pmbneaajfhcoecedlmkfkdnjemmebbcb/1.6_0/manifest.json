{
   "action": {
      "default_popup": "popup.html"
   },
   "browser_action": {
      "default_icon": {
         "128": "icons/icon128.png",
         "16": "icons/icon16.png",
         "48": "icons/icon48.png"
      }
   },
   "content_scripts": [ {
      "js": [ "UnSponsored.js" ],
      "matches": [ "https://www.google.com/*" ]
   } ],
   "description": "Hide sponsored content on web pages.",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "UnSponsored",
   "options_ui": {
      "page": "popup.html"
   },
   "permissions": [ "storage", "activeTab" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.6"
}
